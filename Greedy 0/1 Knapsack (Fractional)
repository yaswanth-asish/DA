#include <stdio.h>

int main() {
    int n;
    printf("");
    scanf("%d",&n);

    float w[n], p[n], ratio[n];

    for(int i=0;i<n;i++)
        scanf("%f%f",&w[i],&p[i]);

    float cap;
    scanf("%f",&cap);

    for(int i=0;i<n;i++)
        ratio[i] = p[i]/w[i];

    // Sort by ratio
    for(int i=0;i<n;i++)
        for(int j=i+1;j<n;j++)
            if(ratio[i] < ratio[j]) {
                float t=ratio[i]; ratio[i]=ratio[j]; ratio[j]=t;
                t=w[i]; w[i]=w[j]; w[j]=t;
                t=p[i]; p[i]=p[j]; p[j]=t;
            }

    float profit=0;
    for(int i=0;i<n;i++) {
        if(cap >= w[i]) {
            cap -= w[i];
            profit += p[i];
        } else {
            profit += ratio[i] * cap;
            break;
        }
    }

    printf("Profit = %f\n", profit);
}
