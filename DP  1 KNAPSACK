#include <stdio.h>

int max(int a, int b){ return (a>b)?a:b; }

int main() {
    int n, cap;
    scanf("%d%d",&n,&cap);

    int w[n], p[n];
    for(int i=0;i<n;i++)
        scanf("%d%d",&w[i],&p[i]);

    int dp[n+1][cap+1];

    for(int i=0;i<=n;i++) {
        for(int j=0;j<=cap;j++) {
            if(i==0 || j==0)
                dp[i][j] = 0;
            else if(w[i-1] <= j)
                dp[i][j] = max(p[i-1] + dp[i-1][j-w[i-1]], dp[i-1][j]);
            else
                dp[i][j] = dp[i-1][j];
        }
    }

    printf("Max Profit = %d\n", dp[n][cap]);
}
